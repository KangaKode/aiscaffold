# {{ project_name }} - Kubernetes ConfigMap
# Generated by aiscaffold v3
#
# Non-secret configuration. Secrets go in a separate Secret resource
# (not generated -- create manually or via CI/CD).

apiVersion: v1
kind: ConfigMap
metadata:
  name: {{ project_slug }}-config
  labels:
    app: {{ project_slug }}
data:
  LOG_LEVEL: "INFO"
{% if persistence == 'sqlite' %}
  DATABASE_PATH: "/app/data/{{ project_slug }}.db"
{% endif %}
